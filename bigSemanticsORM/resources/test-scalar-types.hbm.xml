<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="ecologylab.semantics.compiler.orm.scalartypes.MyEntity" table="my_entity">
		<id name="id" column="id" />
		<property name="name" type="ecologylab.semantics.compiler.orm.scalartypes.MetadataStringHibernateType" column="name" />
		<property name="number" type="ecologylab.semantics.compiler.orm.scalartypes.MetadataIntegerHibernateType" column="number" />
		<bag name="otherNames" lazy="true" table="my_entity__other_names">
			<key column="my_entity_id" not-null="true" />
			<element column="other_name" type="ecologylab.semantics.compiler.orm.scalartypes.MetadataStringBuilderHibernateType" />
		</bag>
	</class>
</hibernate-mapping>

<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="ecologylab.translators.hibernate.TestHibernateXmlMappingGeneratorWithMaps" table="test_map">
    <id name="id" column="id">
      <generator class="identity" />
    </id>
    <map name="scalarScalarMap" lazy="true" table="test_map__map_scalar">
      <key column="test_map_id" not-null="true" />
      <map-key type="java.lang.String" />
      <element column="the_number" type="java.lang.Float" />
    </map>
    <map name="scalarElementMap" lazy="true" table="test_map__map_element">
      <key column="test_map_id" not-null="true" />
      <map-key type="java.lang.String" />
      <many-to-many column="item_id" class="ecologylab.translators.hibernate.TestHibernateXmlMappingGeneratorWithMaps$Item" />
    </map>
  </class>
  <class name="ecologylab.translators.hibernate.TestHibernateXmlMappingGeneratorWithMaps$Item" table="item">
    <id name="id" column="id">
      <generator class="identity" />
    </id>
    <property name="name" />
  </class>
</hibernate-mapping>
